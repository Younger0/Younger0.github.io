{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
     
       "title": {
        "text": "GCSE Results 2000-2021",
        "subtitle":"Proportion of GCSE entries awarded a C/4 or higher in the United Kingdom between 2000 and 2021, by gender  Source: Statista.",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},
    
      "data": {
      "url": "https://raw.githubusercontent.com/Younger0/Younger0.github.io/main/GCSE_results.csv"
    },
    
    "repeat": {"layer": ["Boy", "Girl"]},
   
    "spec": {
        
        "height": 300,
        "width": 320,
 
        "mark": {
            "type":"line",
            "strokeWidth":2},    
  
        "encoding": {
            "x": {
                "field": "Year",
                "type": "temporal"},
    
            "y": {
                "field": {"repeat": "layer"},
                "type": "quantitative",
                "title": null,
                "domain": [40,100]}
            
            
    
            "color": {
                "datum": {"repeat": "layer"},
                "scale": {"range": ["#1B0CE8", "#9C269E"]}}
          } 
      }
  }
