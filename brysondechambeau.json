{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  "title": {
    "text": "Bryson DeChambeau Tournament Positions",
    "subtitle":"Tournament Finishing Position 2020/2021 Season",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
   "data": {
    "url": "https://raw.githubusercontent.com/Younger0/Younger0.github.io/main/brysondechambeau.csv",
    "format": {"type": "csv"}
  },

   "transform":[
      
      {"filter": {
        "field": "player", 
        "oneOf": ["Bryson DeChambeau"]}
        }


    ],
  
  
  "height": 300,
  "width": 310,
   
  "mark": {"type": "line", "point": false},
 
  "encoding": {

    "x": {
      "field": "date",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },

    "y": {
      "field": "Finish",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    },

    "color": {
      "field": "Finish",
      "type": "nominal",
      "scale": {"scheme": "set1"}
    }
}
}
